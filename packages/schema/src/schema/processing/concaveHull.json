{
  "type": "object",
  "description": "ConcaveHull",
  "title": "IConcaveHull",
  "required": ["inputLayer"],
  "additionalProperties": false,
  "properties": {
    "inputLayer": {
      "type": "string",
      "description": "The input layer for Concave Hull."
    },
    "pctconvex": {
      "type": "number",
      "description": "Controls the concaveness of the computed hull, vary from 0 to 1, 0 corresponds to the maximum concavity.",
      "multipleOf": 0.1,
      "minimum": 0,
      "maximum": 1,
      "default": 0.5
    },
    "allowHoles": {
      "type": "boolean",
      "title": "Allow holes in the hull",
      "default": false
    }
  },
  "processGenerationParams": {
    "processName": "concaveHull",
    "processParams": ["pctconvex", "allowHoles"],
    "processLabel": "Concave Hull",
    "processAdditionalsParams": {
      "sql": "SELECT ST_ConcaveHull(geometry,{pctconvex},{allowHoles}) AS geometry, * FROM \"{layerName}\"",
      "gdalFunction": "ogr2ogr"
    },
    "processType": "vector"
  }
}
